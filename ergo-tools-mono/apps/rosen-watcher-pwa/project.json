{
  "name": "rosen-watcher-pwa",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "generators": {},
  "sourceRoot": "apps/rosen-watcher-pwa/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular/build:application",
      "options": {
        "allowedCommonJsDependencies": ["qrcode"],
        "outputPath": "dist/apps/rosen-watcher-pwa",
        "index": "apps/rosen-watcher-pwa/src/index.html",
        "browser": "apps/rosen-watcher-pwa/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/rosen-watcher-pwa/tsconfig.json",
        "assets": [
          "apps/rosen-watcher-pwa/src/favicon.ico",
          "apps/rosen-watcher-pwa/src/assets",
          "apps/rosen-watcher-pwa/src/rosen.html",
          "apps/rosen-watcher-pwa/src/load.html",
          "apps/rosen-watcher-pwa/src/manifest.webmanifest",
          "apps/rosen-watcher-pwa/src/ngsw-worker.js"
        ],
        "styles": [
          "apps/rosen-watcher-pwa/src/variables.css",
          "apps/rosen-watcher-pwa/src/styles.css",
          "apps/rosen-watcher-pwa/src/navigation.css",
          "apps/rosen-watcher-pwa/src/header.css",
          "apps/rosen-watcher-pwa/src/settings.css",
          "apps/rosen-watcher-pwa/src/stats.css",
          "apps/rosen-watcher-pwa/src/app/basewatchercomponent.css",
          "node_modules/@fortawesome/fontawesome-free/css/all.css",
          "apps/rosen-watcher-pwa/src/mat-theme.scss",
          "apps/rosen-watcher-pwa/src/styles_theme-colors.scss"
        ],
        "scripts": [
          "apps/rosen-watcher-pwa/src/rosen-ngsw-worker.js",
          "dist/service/src/lib/rosen-download-worker.js",
          "dist/service/src/lib/rosen-db-worker.js",
          "dist/service/src/lib/chain.service.js",
          "dist/service/src/lib/constants.js",
          "dist/service/src/lib/data.service.js",
          "dist/service/src/lib/storage.service.js",
          "dist/service/src/lib/chain.performance.data.service.js",
          "dist/service/src/lib/reward.data.service.js",
          "dist/service/src/lib/chart.service.js",
          "dist/service/src/lib/download.service.js",
          "dist/service/src/lib/process.event.service.js",
          "dist/service/src/lib/mywatcher.data.service.js",
          "dist/service/src/lib/activepermits.data.service.js",
          "dist/service/src/lib/download.status.indexeddb.service.js"
        ]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2500kb",
              "maximumError": "3mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "serviceWorker": "apps/rosen-watcher-pwa/ngsw-config.json",
          "baseHref": "./"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "serviceWorker": "apps/rosen-watcher-pwa/ngsw-config.json",
          "baseHref": "./"
        }
      },
      "defaultConfiguration": "production",
      "outputs": ["{options.outputPath}"]
    },
    "serve": {
      "executor": "@angular/build:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "rosen-watcher-pwa:build:production"
        },
        "development": {
          "buildTarget": "rosen-watcher-pwa:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular/build:extract-i18n",
      "options": {
        "buildTarget": "rosen-watcher-pwa:build"
      }
    }
  }
}
